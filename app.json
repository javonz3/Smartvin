{
  "expo": {
    "name": "SmartVIN",
    "slug": "smartvin-mobile-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "smartvin",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.smartvin.app"
    },
    "android": {
      "package": "com.smartvin.app"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router", 
      "expo-font", 
      "expo-web-browser", 
      "expo-camera"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}